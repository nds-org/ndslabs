{
    "depends": [
        {
            "key": "elasticsearch2",
            "required": true
        },
        {
            "key": "logstash",
            "required": true
        },
        {
            "key": "logspout",
            "required": true
        }
    ],
    "key": "kibana",
    "isStack": true,
    "isService": true,
    "isPublic": true,
    "isStandalone": true,
    "description": "An open source data visualization platform",
    "label": "Kibana",
    "image": "kibana",
    "ports": [
        { "port": 5601, "protocol": "TCP" }
    ],
    "config":  {
       "ELASTICSEARCH_URL": "http://$(ELASTICSEARCH2_PORT_9200_TCP_ADDR):$(ELASTICSEARCH2_PORT_9200_TCP_PORT)"
    }
}
